    polygon(x = XLimits[c(1, 2, 2, 1)], y = c(YLimits[1] + ((1 / 3) * abs(diff(YLimits))), YLimits[1] + ((1 / 3) * abs(diff(YLimits))), YLimits[1] + ((2 / 3) * abs(diff(YLimits))), YLimits[1] + ((2 / 3) * abs(diff(YLimits)))), col = ThickCrossColour, border = 0)#
