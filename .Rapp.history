    # Turn on vertical bands:#
    LeftBand <- CentreBand <- RightBand <- TRUE#
    # Set band colours:#
    LeftBandColour <- "green"#
    CentreBandColour <- "white"#
    RightBandColour <- "red"#
  }#
  # If plotting Japan:#
  if(Country == "Japan") {#
    # Turn on background and central cricle:#
