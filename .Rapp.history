  Background <- LeftBand <- CentreBand <- RightBand <- TopBand <- MiddleBand <- BottomBand <- CentreCircle <- CentreWheel <- ThickCross <- ThinCross <- ThickSaltire <- ThinSaltire <- FALSE#
